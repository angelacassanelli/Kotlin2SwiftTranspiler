class TestFunctions {
    fun sayHello() {
        println("Hello")
    }

    fun sayHello() { // Errore: Firma duplicata
        println("Duplicate Hello")
    }

    fun add(a: Int, b: Int): Int {
        return a + b
    }

    fun add(a: Int, b: String): Int { // Firma valida perch√© i parametri differiscono
        return a + b.toInt()
    }
}
