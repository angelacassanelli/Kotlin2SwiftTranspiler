class TestFunctions {
    fun sayHello() {
        println("Hello")
    }

    fun sayHello() { // Error: Redeclaration of function 'sayHello'
        println("Duplicate Hello")
    }

    fun add(a: Int, b: Int) {
        println(a+b)
    }

    fun add(a: Int, b: String) { 
        println(a)
        println(b)
    }
}
