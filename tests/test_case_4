class TestVariables {
    fun test() {
        var a: Int = "Wrong type"   // Error: Type mismatch
        
        val b: Int = 10
        var b: Int = 20             // Error: Redeclaration of variable 'b'
        
        var c: String = "test"        
        var c: Boolean = True       // Error: Redeclaration of variable 'c'
    }
}
